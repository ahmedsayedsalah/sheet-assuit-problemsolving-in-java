//code
import java.util.Scanner;
public class Java {
    public static void main(String[] args) {
    Scanner input=new Scanner(System.in);          
    int n;
    n=input.nextInt();
    for(int i=1;i<=12;i++)
          System.out.println(n+" * "+i+" = "+n*i);  

//end
    }
}

//code
import java.util.Scanner;
public class Java {
    // main method
    public static void main(String[] args) {
        
    //class used to enter numbers
    Scanner input=new Scanner(System.in); 
    
    int n=input.nextInt();
    for(int i=1;i<=n;i++) //numbers of digits  
    {
        int a=input.nextInt();
        if(a==0)
            System.out.println(0);
        while(a!=0) //seprate the numbers about its some 
        {
            System.out.print(a%10+" ");
            a/=10;
        }
        System.out.println("");
    }
//end
    }
}

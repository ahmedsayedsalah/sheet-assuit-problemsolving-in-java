//code
import java.util.Scanner;
public class Java {
    // main method
    public static void main(String[] args) {
        
    //class used to enter numbers
    Scanner input=new Scanner(System.in); 
    
    int n=input.nextInt();
    for(int i=1;i<=n;i++)
    {
        for(int j=1;j<=i;j++)
            System.out.print("*");
    System.out.print("\n");
    }
//end
    }
}

import java.util.Scanner;
public class Java {
    public static void main(String[] args) {
        Scanner input=new Scanner(System.in);          
        int n;
        n=input.nextInt();
        if(n==1) //n=هنا يوجد حاله واجده مفيش فيها اعداد زوجيه وهي عندما 1
            System.out.println("-1");
        else{
        for(int i=2;i<=n;i+=2) //cases زودنا ال عداد ب 2 عشان نقلل من عدد ال 
        {
            System.out.println(i);
        }
        }
      //end
    }
}

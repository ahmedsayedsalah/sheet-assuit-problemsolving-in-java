//code
import java.util.Scanner;
public class Java {
    // main method
    public static void main(String[] args) {
        
    //class used to enter numbers
    Scanner input=new Scanner(System.in); 
    
    int a=input.nextInt(),b=input.nextInt();
    if(a-b>=0)
         System.out.println(a-b);
    else 
        System.out.println(0);
//end
    }
}

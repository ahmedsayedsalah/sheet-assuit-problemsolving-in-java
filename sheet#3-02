//code
// for scanner , Arrays and collections
import java.util.*;

public class Java {
    // main method
    public static void main(String[] args) {
        
    //class used to enter numbers
    Scanner input=new Scanner(System.in); 
    
    int n=input.nextInt();
    int[] arr=new int[n];
        for (int i = 0; i < arr.length; i++) {
            arr[i]=input.nextInt();
        }
        int x=input.nextInt();
        boolean c=false;
        for (int i = 0; i < arr.length; i++) {
            if(x==arr[i])
            {
                System.out.print(i);
                c=true;
                break;
            }
        }
        if(c==false)
            System.out.println("-1");
//end
    }
    
}

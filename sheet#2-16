//code
//library contains all of functions that you may use
#include <bits/stdc++.h>
using namespace std;
//main function
int main()
{
    int n;
    cin>>n;
    for(int k=n;k>=1;k--)
        {
            for(int j=1;j<=k;j++)
            cout<<"*";
        cout<<endl;
        }
   return 0;
}

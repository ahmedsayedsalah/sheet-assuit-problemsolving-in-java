//code
import java.util.Scanner;
public class Java {
    // main method
    public static void main(String[] args) {
        
    //class used to enter numbers
    Scanner input=new Scanner(System.in); 
    
    int n=input.nextInt();
    for(int k=n;k>=1;k--)
        {
            for(int j=1;j<=k;j++)
               System.out.print("*");
        System.out.println("");
        }
//end
    }
}

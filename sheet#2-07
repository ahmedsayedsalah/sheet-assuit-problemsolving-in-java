//code
import java.util.Scanner;
public class Java {
    public static void main(String[] args) {
    Scanner input=new Scanner(System.in);          
    int n=input.nextInt();
   for(int i=1;i<=n;i++)
   {
    int a=input.nextInt();
    long fact=1;                        //int قيمة المضروب تتخطي       
    for(int j=1;j<=a;j++)               //يجب اعطاء المضروب قيمة 1 بعد ما اللوب تنتهي لان لكل عدد مضروب خاص به
           fact*=j;
     System.out.println(fact);
   } 

//end
    }
}
